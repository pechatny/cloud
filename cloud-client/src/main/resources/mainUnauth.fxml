<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="550.0" prefWidth="549.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="ru.pechatny.cloud.client.Controllers.UnauthorizedController">
