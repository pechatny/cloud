                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                    <children>
                        <MenuBar layoutY="4.0" prefHeight="29.0" prefWidth="524.0" AnchorPane.bottomAnchor="0.0"
